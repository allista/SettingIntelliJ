<application>
  <component name="EditorSettings">
    <option name="STRIP_TRAILING_SPACES" value="None" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="important" pattern="\bNotImplementedException\b" />
    <pattern icon="important" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;BUG)(\W|$)(.*)" />
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;TODO)(\W|$)(.*)" />
    <pattern pattern="(?&lt;!^\s*//.+//)(?&lt;TAG&gt;DEBUG)(\W|$)(.*)" />
    <filter name="debug">
      <pattern index="3" />
    </filter>
  </component>
</application>