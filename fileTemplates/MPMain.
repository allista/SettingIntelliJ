from BioUtils.Tools.Multiprocessing import MPMain

class Main(MPMain):
    def _main(self):
        self.argument('arg', type=str,
                      help='Positional argument.')
        self.parse_args()
        

if __name__ == '__main__':
    Main(run=True)
